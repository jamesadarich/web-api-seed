{
  "name": "web-api-seed",
  "version": "1.0.0",
  "description": "seed to build a web API",
  "main": "build/server/startup.js",
  "scripts": {
    "build": "tsc && npm run build-emails",
    "build-emails": "mjml ./server/emails/*.mjml --output ./server/emails",
    "pretest": "npm run build",
    "test": "alsatian \"./build/test/**/*.spec.js\"",
    "dev": "nodemon",
    "dev-start": "ts-node --inspect=5858 server/startup.ts",
    "prestart": "npm run build",
    "start": "node build/server/startup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesrichford/web-api-seed.git"
  },
  "keywords": [
    "web",
    "api",
    "seed"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesrichford/web-api-seed/issues"
  },
  "homepage": "https://github.com/jamesrichford/web-api-seed#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/body-parser": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.35",
    "@types/express-session": "0.0.32",
    "@types/handlebars": "^4.0.32",
    "@types/nodemailer": "^1.3.32",
    "@types/passport": "^0.3.2",
    "@types/passport-http": "^0.3.2",
    "alsatian": "^2.1.0",
    "mjml": "^3.3.0",
    "nodemon": "^1.14.11",
    "ts-node": "^4.1.0",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.16.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "handlebars": "^4.0.6",
    "inversify": "^3.0.0",
    "inversify-express-utils": "^3.2.0",
    "mssql": "^3.3.0",
    "nodemailer": "^3.1.7",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "reflect-metadata": "^0.1.9",
    "typeorm": "0.0.9"
  }
}
